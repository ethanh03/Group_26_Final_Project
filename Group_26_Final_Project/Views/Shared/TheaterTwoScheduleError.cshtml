@model IEnumerable<String>

<h1>Error</h1>

<p>Something went wrong whilst creating the schedule:</p>

@if (Model != null)
{
    foreach (String message in Model)
    {
        <p><span class="text-danger">@message</span></p>
    }
}

<a asp-action="UpcomingScheduleTheaterTwo" asp-controller="Showtimes" class="btn btn-success">Back to Schedule</a>
